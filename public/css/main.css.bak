/**SideBar***/

#sideBar {
  width: 15px;
  height:100px;
  position: fixed;
  padding-right:5px;
  right:0px;
  top:230px;
 
}
#sideBar h5 {
  writing-mode:tb-rl;
  cursor:pointer;
}
#sideContent {
	background:yellow;
}
#bottomBar{
	position:fixed;
	bottom:10px;
	right:5px;
}

/**** Infinite Scroll ****/

#infscr-loading {
  position: fixed;
  text-align: center;
  bottom: 30px;
  left: 42%;
  z-index: 100;
  background: grey;
  background: hsla( 0, 50%, 100%, 0.9 );
  padding: 20px;
  color: #222;
  font-size: 15px;
  font-weight: bold;
  -webkit-border-radius: 10px;
     -moz-border-radius: 10px;
          border-radius: 10px;
}

