/****Channel***/
#channelBanner
{
	margin:30px auto; 
	background:	#FFE1FF; 
	padding:5px 0px;	
	text-align:center;
	background:url(/public/images/channel_bannerbg.png);
	color:#8B8B83;
}

#channelBanner a
{
	border:1px solid #D8BFD8;
	padding:2px 20px;
	background:url(/public/images/channel_bannerbg2.png);
	color:#DC143C;
}

#channelBanner a:hover
{

	text-shadow: rgba(0, 0, 0, 0.25) 0 1px 0;
	-moz-box-shadow: 0px 2px 2px grey; /* Firefox */
	-webkit-box-shadow: 0px 2px 2px grey; /* Safari and Chrome */
	box-shadow: 0px 1px 2px grey; /* Opera 10.5+, future browsers and IE6+ using IE-CSS3 */ 
}


#channelBanner a.active
{

	text-shadow: rgba(0, 0, 0, 0.25) 0 1px 0;
	-moz-box-shadow: 0px 2px 2px grey; /* Firefox */
	-webkit-box-shadow: 0px 2px 2px grey; /* Safari and Chrome */
	box-shadow: 0px 1px 2px grey; /* Opera 10.5+, future browsers and IE6+ using IE-CSS3 */ 
}

.image-grid {
	margin: 10px;
	padding-left: 10px;
}
.image-grid:after {
	content: "";
	display: block;
	height: 0;
	overflow: hidden;
	clear: both;
}
.image-grid li {
	width: 80px;
	margin: 20px 0 0 20px;
	float: left;
	text-align: center;
	font-family: "Helvetica Neue", sans-serif;
	line-height: 17px;
	color: #686f74;
	height: 140px;
	overflow: hidden;
}
.image-grid li img{
	width:80px;	
	height:80px;
}
.image-grid li img,
.image-grid li strong {
	display: block;
}
.image-grid li strong {
	color: grey;
}

